<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
    <configuration>
        <!-- document encoding -->
        <encoding>UTF-8</encoding>

        <!-- title of the document -->
        <documentTitle>Rules-Engine ER</documentTitle>

        <!-- report name - displayed at top-nav -->
        <reportName>
            <![CDATA[
                <img src='EL.png'/>Rules-Engine Extent Report
            ]]>
        </reportName>

        <!-- report headline - displayed at top-nav, after reportHeadline -->
        <reportHeadline></reportHeadline>

        <!-- custom javascript -->
        <scripts>
            <![CDATA[
                $(document).ready(function() {
                    $("nav div a span:first").text("Rules Engine ER");
                });
            ]]>
        </scripts>

        <!-- custom styles -->
        <styles>
            <![CDATA[
                .report-name { padding-left: 15px; } .report-name > img { width: 76px !important;float: left;height: 90%;margin-left: 438px !important;margin-top: 2px;width: auto; }
            ]]>
        </styles>
    </configuration>
</extentreports>